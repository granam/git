{
    "name": "granam/git",
    "license": "MIT",
    "type": "library",
    "description": "Basic workflow with GIT",
    "keywords": [
        "git",
        "versions",
        "branch",
        "tag",
        "checkout",
        "clone"
    ],
    "autoload": {
        "psr-4": {
            "Granam\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Granam\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1",
        "granam/strict-object": "^3.0"
    },
    "require-dev": {
        "granam/exceptions-hierarchy": "^5.0",
        "phpunit/phpunit": "^9.4",
        "symfony/process": "^5.1"
    },
    "config": {
        "sort-packages": true
    }
}
