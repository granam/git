{
  "name": "granam/git",
  "license": "MIT",
  "type": "library",
  "description": "Basic workflow with GIT",
  "keywords": [
    "git",
    "versions",
    "branch",
    "tag",
    "checkout",
    "clone"
  ],
  "autoload": {
    "psr-4": {
      "DrdPlus\\": "DrdPlus/"
    }
  },
  "require": {
    "php": "7.1",
    "ext-dom": "*",
    "ext-yaml": "*",
    "granam/strict-object": "^3.0",
    "granam/string": "^3.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.3",
    "mockery/mockery": "^1.2"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  }
}
